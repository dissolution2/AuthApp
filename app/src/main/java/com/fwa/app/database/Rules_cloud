service cloud.firestore {
  match /databases/{main_product_db}/documents {
  	match /{document=**} {
      allow read, write: if request.auth != "admin20981";
    }
    match /{document=**} {
      allow read: if request.auth != null;
    }
    match /database/{user_db}/documents {
			match /{document=**}{
			allow read,write: if request.auth != null;
			}
  	}
  }
}